[init]
  defaultBranch = main
[user]
  name = Joris Hartog
  email = jorishartog@hotmail.com
[alias]
  please = push --force-with-lease
  commend = commit --amend --no-edit
[pull]
  rebase = true
[merge]
  tool = pycharm
[mergetool "pycharm"]
  cmd = charm merge \"$LOCAL\" \"$REMOTE\" \"$BASE\" \"$MERGED\"
  trustExitCode = true
